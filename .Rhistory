library(shiny)
# Define the UI (User Interface)
ui <- fluidPage(
# Application title
titlePanel("Hello World Shiny App with Images"),
# Create a button and an output area
sidebarLayout(
sidebarPanel(
# Action button to trigger the message and image
actionButton("hello_button", "Press Me!")
),
mainPanel(
# Text output area to show the message
textOutput("hello_text"),
# Image output area to display the image
uiOutput("hello_image")
)
)
)
# Define server logic required to respond to the button click
server <- function(input, output) {
# Reactive expression to respond to button clicks
output$hello_text <- renderText({
# Display text when button is clicked
if(input$hello_button > 0) {
return("Hello, World!")
}
})
# Display an image after the button is clicked
output$hello_image <- renderUI({
# If button is clicked, display an image
if(input$hello_button > 0) {
tags$img(src = "path_to_image.jpg", width = "400px", height = "300px")
# Alternatively, if using a URL image
# tags$img(src = "https://www.example.com/image.jpg", width = "400px", height = "300px")
}
})
}
# Run the application
shinyApp(ui = ui, server = server)
# Load the Shiny package
library(shiny)
# Define the UI (User Interface)
ui <- fluidPage(
# Application title
titlePanel("Drag and Drop Image to Display"),
# Create an input area for file upload (drag-and-drop supported)
sidebarLayout(
sidebarPanel(
# File input for uploading image, allowing drag-and-drop
fileInput("image_upload", "Drag and Drop an Image Here",
accept = c('image/png', 'image/jpeg', 'image/jpg'),
buttonLabel = "Browse...",
placeholder = "No image selected"),
# Display instructions
helpText("You can drag and drop an image file here, or click Browse to select an image.")
),
mainPanel(
# Display uploaded image
uiOutput("image_display")
)
)
)
# Define server logic required to display the uploaded image
server <- function(input, output) {
# React to the image file input
output$image_display <- renderUI({
# Check if a file is uploaded
req(input$image_upload)
# Display the image using img tag
tags$img(src = input$image_upload$datapath,
width = "400px", height = "auto")
})
}
# Run the application
shinyApp(ui = ui, server = server)
